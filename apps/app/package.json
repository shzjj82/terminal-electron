{
  "name": "terminal",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Terminal Electron <support@terminal-electron.com>",
  "homepage": "https://electron-vite.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/shzjj82/terminal-electron.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps || echo 'Some native dependencies failed to build, but continuing...'",
    "build:unpack": "electron-vite build && electron-builder --dir --publish=never",
    "build:win": "electron-vite build && electron-builder --win --publish=never",
    "build:win:x64": "electron-vite build && electron-builder --win --x64 --publish=never",
    "build:win:arm64": "electron-vite build && electron-builder --win --arm64 --publish=never",
    "build:mac": "electron-vite build && electron-builder --mac --publish=never",
    "build:mac:arm64": "electron-vite build && electron-builder --mac --arm64 --publish=never",
    "build:mac:x64": "electron-vite build && electron-builder --mac --x64 --publish=never",
    "build:linux": "electron-vite build && electron-builder --linux --publish=never",
    "build:linux:arm64": "electron-vite build && electron-builder --linux --arm64 --publish=never",
    "build:linux:x64": "electron-vite build && electron-builder --linux --x64 --publish=never",
    "build:all": "yarn build && electron-builder -mwl --publish=never",
    "build:all:arm64": "yarn build && electron-builder -mwl --arm64 --publish=never",
    "build:all:x64": "yarn build && electron-builder -mwl --x64 --publish=never",
    "clean": "rm -rf out dist release"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/ssh2": "^1.15.5",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.3.9",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.294.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react-router-dom": "^7.6.3",
    "react-terminal-ui": "^1.4.0",
    "ssh2": "^1.16.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "build": {
    "appId": "com.terminal.electron",
    "productName": "Terminal Electron",
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*",
      "node_modules/**/*"
    ],
    "electronVersion": "37.2.3",
    "mac": {
      "category": "public.app-category.developer-tools",
      "artifactName": "Terminal.Electron-${arch}.${ext}",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "build/icon.ico",
      "artifactName": "Terminal.Electron.Setup-${arch}.${ext}"
    },
    "linux": {
      "maintainer": "Terminal Electron <support@terminal-electron.com>",
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "deb",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "Terminal.Electron.Setup-${arch}.${ext}"
    }
  }
}
